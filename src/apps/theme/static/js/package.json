  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@tailwindcss/forms": "^0.2.1",
    "autoprefixer": "^10.2.4",
    "clean-css-cli": "^4.3.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.1.9",
    "postcss-cli": "^8.3.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.1"
  }
}
